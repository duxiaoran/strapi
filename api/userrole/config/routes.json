{
  "routes": [
    {
      "method": "GET",
      "path": "/userroles",
      "handler": "Userrole.find",
      "config": {
        "policies": ["plugins.users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/userroles/count",
      "handler": "Userrole.count",
      "config": {
        "policies": ["plugins.users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/userroles/:id",
      "handler": "Userrole.findOne",
      "config": {
        "policies": ["plugins.users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/userroles",
      "handler": "Userrole.create",
      "config": {
        "policies": ["plugins.users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/userroles/:id",
      "handler": "Userrole.update",
      "config": {
        "policies": ["plugins.users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/userroles/:id",
      "handler": "Userrole.delete",
      "config": {
        "policies": ["plugins.users-permissions.isAuthenticated"]
      }
    }
  ]
}
